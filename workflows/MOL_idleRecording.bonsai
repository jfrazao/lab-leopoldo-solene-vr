        <ExtentX>1.35</ExtentX>
        <ExtentY>1</ExtentY>
        <LocationX>2</LocationX>
        <LocationY>0.5</LocationY>
        <Layer>0</Layer>
        <Angle>0</Angle>
      </Expression>
      <Expression xsi:type="SubscribeSubject">
        <Name>DrawStimuli</Name>
      </Expression>
      <Expression xsi:type="Condition">
